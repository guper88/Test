{
  "name": "Someones extension",
  "author": "Someone",
  "sources": [
    {
      "name": "limetor",
      "version": 1,
      "baseUrl": "https://www.limetorrents.lol/home",
      "htmlParser": {
        "searchUrl": "/search/{query}/1/",
        "rows": ".table-list tbody tr",
        "magnet": {
          "externalLinkQuery": ".name a:nth-last-child(1)",
          "query": "div.clearfix > ul:nth-child(1) > li:nth-child(1) > a",
          "attribute": "href"
        },
        "title": {
          "query": ".name > a:nth-last-child(1)"
        },
        "size": {
          "query": ".size",
          "regex": "(.*?B)"
        },
        "sl": {
          "seeders": ".seeds",
          "leechers": ".leeches"
        }
      }
    }
  ]
}
